<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
    
 	<navigation-rule>
 		<from-view-id>index2.xhtml</from-view-id>
 		<navigation-case>
 			<from-outcome>entrar</from-outcome>
 			<to-view-id>./exito.xhtml</to-view-id>
 		</navigation-case>
 		<navigation-case>
 			<from-outcome>salir</from-outcome>
 			<to-view-id>./fallo.xhtml</to-view-id>
 		</navigation-case>
 		<navigation-case>
 			<from-outcome>nada</from-outcome>
 			<to-view-id>index2.xhtml</to-view-id>
 		</navigation-case>
 	</navigation-rule>
 	
</faces-config>